{
  "name": "cozyevent",
  "version": "1.3.0",
  "description": "World's fastest, Lightweight Async/Sync Event Emitter",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "benchmark:emit": "node benchmark/cozy.emit.benchmark.js",
    "benchmark:on": "node benchmark/cozy.on.benchmark.js",
    "benchmark:removeAllLiseners": "node benchmark/cozy.removeAllListeners.benchmark.js",
    "benchmark": "npm run benchmark:emit && npm run benchmark:on && npm run benchmark:removeAllLiseners"
  },
  "keywords": [
    "EventEmitter",
    "Events",
    "emit",
    "addEventListener",
    "async",
    "sync",
    "EventEmitter3",
    "CozyEvent",
    "once",
    "on",
    "EventListerner",
    "Typescript",
    "fast",
    "lightweight",
    "benchmark",
    "eventemitter2"
  ],
  "author": "M. Ergin Turk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RecursiveVoid/CozyEvent.git"
  },
  "devDependencies": {
    "@braintree/event-emitter": "^2.0.2",
    "@protobufjs/eventemitter": "^1.1.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-wasm": "^6.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/react": "^19.0.12",
    "backbone": "^1.6.0",
    "benchmark": "^2.1.4",
    "emitix": "^2.3.2",
    "event-emitter": "^0.3.5",
    "eventemitter2": "^6.4.9",
    "eventemitter3": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rollup": "^4.27.4",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tseep": "^1.3.1",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "type": "module",
  "files": [
    "dist/",
    "!dist/core/"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  }
}
